
public class ArmstrongNumber {
	public static void main(String[] args) {
	int num=153;
	int temp;
	temp=num;
	int sum=0,rem;
	while(num!=0) {
		rem=num%10;
		sum=sum+rem*rem*rem;
		num=num/10;
	}
	if(sum==temp) {
		System.out.println("Armstrong number");
	}
	else {
		System.out.println("not a Armstrong number");
	}
}
}
