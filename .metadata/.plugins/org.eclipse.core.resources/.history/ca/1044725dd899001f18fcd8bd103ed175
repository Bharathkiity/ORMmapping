//natural number >1
//two factors  and itself
public class PrimeNumber {
	public static void main(String[] args) {
	int num=88;
	boolean prime=true;
	//int count=0;
	if(num>1) {
		for(int i=1;i<=num;i++) {
			if(num%i==0) 
			//count++;
				prime=false;
			break;
			}
		System.out.println(prime);
		}

	}
}
